{
  "name": "bollinger",
  "version": "0.0.0-development",
  "description": "An open-source technical analysis library written with TypeScript.",
  "main": "src/index.ts",
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@commitlint/prompt": "^8.0.0",
    "@semantic-release/changelog": "^3.0.4",
    "@semantic-release/git": "^7.0.12",
    "@types/jest": "^24.0.15",
    "@typescript-eslint/eslint-plugin": "^1.10.2",
    "@typescript-eslint/parser": "^1.10.2",
    "commitizen": "^3.1.1",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^2.4.1",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "semantic-release": "^15.13.16",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.2"
  },
  "scripts": {
    "commit": "npx git-cz",
    "lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "test": "jest --forceExit --coverage --verbose",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/phmatray/bollinger.git"
  },
  "keywords": [
    "bollinger",
    "technical analysis"
  ],
  "author": "Philippe Matray <phmatray@gmail.com>",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/phmatray/bollinger/issues"
  },
  "homepage": "https://github.com/phmatray/bollinger#readme",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
